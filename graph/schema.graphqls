scalar Time

type Event {
  id: ID!
  dateStart: Time!
  dateEnd: Time!
  status: Boolean!
  priority: Int!
  group: Int!
  eventName: String!
  description: String!
  user: User!
}

type User {
  id: ID!
  name: String!
}

type Query {
  events: [Event!]!
}

input NewEvent {
  eventName: String!
  description: String!
  dateStart: Time!
  dateEnd: Time!
}

type Mutation {
  createEvent(input: NewEvent!): Event!
}
